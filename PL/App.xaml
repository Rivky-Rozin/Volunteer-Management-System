<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="LoginWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles.xaml"/>
                
            </ResourceDictionary.MergedDictionaries>
            <local:VolunteerSelectMenuCollection x:Key="VolunteerSelectMenuCollectionKey" />
            <local:CallTypeCollection x:Key="CallTypeCollectionKey" />
            <local:UpdateToTrueConverter x:Key="ConvertUpdateToTrueKey" />
            <local:UpdateToVisibleConverter x:Key="ConvertUpdateToVisibleKey" />
            <local:DistanceKindCollection x:Key="DistanceKindCollectionKey" />
            <local:VolunteerRoleCollection x:Key="VolunteerRoleCollectionKey" />
            <local:CallInListFieldCollection x:Key="CallInListCollectionKey" />
            <local:CallStateToGeneralEditabilityConverter x:Key="GeneralEditConverter"/>
            <local:CallStateToMaxTimeEditabilityConverter x:Key="MaxTimeEditConverter"/>
            <local:CollectionToVisibilityConverter x:Key="CollectionVisibilityConverter"/>

        </ResourceDictionary>
    </Application.Resources>
</Application>

